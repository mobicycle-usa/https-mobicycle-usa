{
  "name": "https-mobicycle-usa",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bunx wrangler dev --live-reload",
    "build": "mkdir -p dist && bunx esbuild _worker.js --bundle --format=esm --outfile=dist/_worker.js --external:node:* && bunx @tailwindcss/cli -i ./src/input.css -o ./dist/output.css --minify && cp public/* dist/",
    "deploy": "bunx wrangler deploy",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "hono": "^4.6.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241004.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.3",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.9",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "typescript": "^5.6.2",
    "wrangler": "^4.36.0"
  }
}