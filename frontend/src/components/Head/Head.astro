---
interface Props {
	title: string;
}

const { title: pageTitle } = Astro.props; 
---

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/logo.png" />
    <meta name="generator" content={Astro.generator} />
    <title>{pageTitle}</title>    
</head>